modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_gemini==1.0.0", "python_openai==1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["file", "freetype", "gumbo", "harfbuzz", "jbig2dec", "lcms2", "libimagequant", "libjpeg", "libjpeg_turbo", "libtiff", "libwebp", "libxcrypt", "mupdf", "openjpeg", "swig", "tcl", "tesseract", "tk", "xcbuild", "zlib"]

[workflows]
